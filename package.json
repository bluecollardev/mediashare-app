    "@react-native-community/cli-platform-android": "^9.0.0",
    "@react-native-community/cli-platform-ios": "^9.0.0",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/jest": "25.2.3",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-navigation": "latest",
    "@types/react-redux": "7.1.23",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-form": "8.3.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "25.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "deprecated-react-native-prop-types": "^4.0.0",
    "eslint": "6.5.1",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "^0.73.6",
    "react-native-cli": "^2.0.1",
    "react-native-dotenv": "^3.4.6",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "2.13.8",
    "remote-redux-devtools": "0.5.16",
    "remote-redux-devtools-on-debugger": "0.8.3",
    "typescript": "^4.6.3"
  },
  "jest": {
