{
  "name": "mediashare",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "start": "expo start --dev-client --port 8081",
    "postinstall": "patch-package && yarn util:ios:refresh-pods",
    "android:local": "react-native run-android --variant=debug",
    "android:staging": "react-native run-android --variant=staging",
    "android:prod": "react-native run-android --variant=release",
    "android:logger": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "ios:local": "react-native run-ios --scheme 'mediashare-development' --simulator='iPhone 12 Pro'",
    "ios:staging": "react-native run-ios --scheme 'mediashare-production' --simulator='iPhone 12 Pro'",
    "ios:prod": "react-native run-ios --scheme 'mediashare-production' --simulator='iPhone 12 Pro'",
    "test": "jest",
    "util:bump-version": "npx react-native-version --never-amend",
    "util:ios:clean-pods": "cd ios && bash ./purge-pod-artifacts.sh && cd ..",
    "util:ios:install-pods": "cd ios && pod install --clean-install && cd ..",
    "util:ios:refresh-pods": "yarn util:ios:clean-pods && yarn util:ios:install-pods",
    "util:modules:rebuild": "rm -rf ./node_modules && yarn install",
    "util:link-react-native-config": "react-native link react-native-config",
    "util:lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "util:sort-pkg": "npx sort-package-json",
    "util:update": "git pull --rebase",
    "util:adb:map": "adb reverse tcp:5000 tcp:5000",
    "invite-ios:bcdevlucas": "npx uri-scheme open mediashare://accept-invitation/6190693aa0c0e20021fa2324 --ios",
    "invite-ios:armos51": "npx uri-scheme open mediashare://accept-invitation/61907743a0c0e20021fa232f --ios",
    "invite-ios:test": "npx uri-scheme open mediashare://accept-invitation/637147fefb4dfea595826d8d --ios",
    "invite-android:bcdevlucas": "npx uri-scheme open mediashare://accept-invitation/6190693aa0c0e20021fa2324 --android",
    "invite-android:armos51": "npx uri-scheme open mediashare://accept-invitation/61907743a0c0e20021fa232f --android",
    "invite-android:test": "npx uri-scheme open mediashare://accept-invitation/637147fefb4dfea595826d8d --android"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.2",
    "@expo/vector-icons": "12.0.2",
    "@expo/webpack-config": "^0.17.3",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^6.0.0",
